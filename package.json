{
  "name": "streamdeck-homeassistant-vue",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rollup --configPlugin typescript -c && vite build",
    "build-dev": "rollup --configPlugin typescript -c -w --watch.onEnd=\"vite build --sourcemap inline --minify false && streamdeck restart de.perdoctus.streamdeck.homeassistant\"",
    "validate": "streamdeck validate --no-update-check ./de.perdoctus.streamdeck.homeassistant.sdPlugin",
    "bundle": "streamdeck bundle --no-update-check ./de.perdoctus.streamdeck.homeassistant.sdPlugin",
    "type-check": "tsc && vue-tsc -p src/view/tsconfig.json",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "ci-lint": "eslint . && prettier -c . && tsc && vue-tsc -p src/view/tsconfig.json",
    "postinstall": "patch-package",
    "test": "vitest --run"
  },
  "dependencies": {
    "@elgato/streamdeck": "^1.3.0",
    "@mdi/js": "^7.4.47",
    "@svgdotjs/svg.js": "^3.2.4",
    "axios": "1.8.2",
    "bootstrap": "^5.3.3",
    "home-assistant-js-websocket": "^9.4.0",
    "js-yaml": "^4.1.0",
    "nunjucks": "^3.2.4",
    "svgdom": "^0.1.19",
    "vue": "^3.5.13",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@elgato/cli": "^1.1.1",
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.20.0",
    "@modyfi/vite-plugin-yaml": "^1.1.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-yaml": "^4.1.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/bootstrap": "^5.2.10",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.17.17",
    "@types/nunjucks": "^3.2.6",
    "@types/svgdom": "^0.1.2",
    "@types/ws": "^8.5.14",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.6",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.32.0",
    "fast-glob": "^3.3.3",
    "jiti": "^2.4.2",
    "patch-package": "^8.0.0",
    "prettier": "^3.4.2",
    "rollup-plugin-cleandir": "^3.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.84.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vite": "^5.4.14",
    "vitest": "^3.0.5",
    "vitest-mock-extended": "^2.0.2",
    "vue-tsc": "^2.2.0"
  }
}
