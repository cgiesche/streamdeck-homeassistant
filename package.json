{
  "name": "streamdeck-homeassistant-vue",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "validate": "streamdeck validate --no-update-check ./de.perdoctus.streamdeck.homeassistant.sdPlugin",
    "build-dev": "vite build --watch --sourcemap inline --minify false",
    "bundle": "streamdeck bundle --no-update-check ./de.perdoctus.streamdeck.homeassistant.sdPlugin",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@mdi/font": "^7.1.96",
    "@mdi/js": "^7.1.96",
    "@popperjs/core": "^2.11.8",
    "axios": "1.7.4",
    "bootstrap": "^5.3.2",
    "core-js": "^3.26.1",
    "js-yaml": "^4.1.0",
    "nunjucks": "3.2.4",
    "snapsvg-cjs": "^0.0.6",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@elgato/cli": "^1.1.0",
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@modyfi/vite-plugin-yaml": "^1.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/bootstrap": "^5.2.10",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "~20.17.11",
    "@types/nunjucks": "^3.2.6",
    "@types/snapsvg": "^0.5.8",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.2.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.4.2",
    "sass": "^1.83.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "vite": "^5.4.11",
    "vue-tsc": "^2.2.0"
  }
}
