{
  "name": "streamdeck-homeassistant-vue",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "validate": "streamdeck validate --no-update-check ./de.perdoctus.streamdeck.homeassistant.sdPlugin",
    "build-dev": "vite build --watch --sourcemap inline --minify false",
    "bundle": "streamdeck bundle --no-update-check ./de.perdoctus.streamdeck.homeassistant.sdPlugin",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@mdi/js": "^7.4.47",
    "@popperjs/core": "^2.11.8",
    "axios": "1.11.0",
    "bootstrap": "^5.3.8",
    "core-js": "^3.45.1",
    "js-yaml": "^4.1.0",
    "nunjucks": "3.2.4",
    "snapsvg-cjs": "^0.0.6",
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.1.1",
    "@rushstack/eslint-patch": "^1.12.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.35.0",
    "eslint-plugin-vue": "^10.4.0",
    "prettier": "^3.6.2",
    "sass": "^1.68.0",
    "vite": "^7.1.5",
    "@elgato/cli": "^1.0.1"
  }
}
